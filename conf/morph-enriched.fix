## 		<!-- Prepare "Gemeindeschlüssel" for lookup in geonames map and "ags" -->
## 		<choose>
## 			<data source="gemeindekennzahl" name="@ags">
## 				<regexp match="(\d{5})(\d{4})(\d{3})" format="${1}${3}" />
