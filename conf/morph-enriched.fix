## 		<!-- API descriptions, see #421 -->
## 		<combine name="@serviceUrl" value="${a}" sameEntity="true" reset="true" flushWith="009Q.z" >
## 			<data source="009Q.x">
## 				<not-equals string="Weitere Informationen"/>
